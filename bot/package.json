{
  "name": "sougeisha-bot",
  "version": "1.0.0",
  "private": true,
  "description": "送迎者bot (Discord.js v14)",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "deploy:guild": "node script/deployGuildCommands.js",
    "deploy:global": "node script/deployGlobalCommands.js",
    "deploy": "node script/deploy.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "freeze:guard": "grep -r 'new EmbedBuilder' handler command --exclude-dir=共通 --exclude=Embedテンプレート.js || echo '✅ Freeze Compliance: No legacy EmbedBuilders found.'"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.12.0",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}